import java.util.Scanner;
class Main
{
    public static void main(String[] args)
    {
        Scanner s = new Scanner(System.in);
        int n=s.nextInt();
        if(n>=-9 &&n<=9)
        {
            System.out.println(n+" is a single digit");
        }
        else
        {
            System.out.println(n+" is not an single digit");
        }
        s.close();
    }
}
